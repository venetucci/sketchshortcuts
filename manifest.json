{
  "manifest_version": 2,

  "name": "Sketch Shortcuts - New Tab Replacement",
  "short_name": "Sketch Shortcuts",
  "description": "Practical Sketch Shortcuts shown on your new tab page.",
  "version": "0.0.1",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  }
}
